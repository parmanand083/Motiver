<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="in.motivation.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="in.motivation.ui.dashboard.DashboardFragment"
        android:label="Videos"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_quotes"
        android:name="in.motivation.ui.Quotes.QuotesFragment"
        android:label="Quotes"
        tools:layout="@layout/fragment_more" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="in.motivation.ui.more.MoreFragment"
        android:label="More"
        tools:layout="@layout/quotes_fragment" />
</navigation>